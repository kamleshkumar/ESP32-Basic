/***
 * LED Blinking
 * ESP32 onboard LED Blink test
 */
#include <Arduino.h>
const int ledPin = 2;

void setup() {
  Serial.begin(115200);  // â† THIS WAS MISSING!
  
  // setup pin 2 as a digital output pin (ESP32 onboard LED)
  pinMode(ledPin, OUTPUT);
  
  Serial.println("ğŸš€ LED Blink Test Started");
}

void loop() {
  digitalWrite(ledPin, HIGH);  // turn on the LED
  Serial.println("ğŸ’¡ HIGH");   // Added println for new lines
  delay(500);                  // wait for half a second
  
  digitalWrite(ledPin, LOW);   // turn off the LED
  Serial.println("ğŸ”Œ LOW");    // Added println for new lines
  delay(500);                  // wait for half a second
}
